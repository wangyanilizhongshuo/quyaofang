<view class="city-select"><scroll-view class="city-select-main" scroll-top="{{scrollTop}}" scroll-y="true" id="city-select-main" scroll-into-view="{{toView}}"><block tt:if="{{activeCity&&!serachCity}}"><view class="hot-title">当前定位城市</view></block><block tt:if="{{activeCity&&!serachCity}}"><view class="hot-city"><view data-event-opts="{{[['tap',[['cityTrigger',['$0'],['activeCity']]]]]}}" class="hot-item" bindtap="__e">{{activeCity[formatName]}}</view></view></block><block tt:if="{{hotCity.length>0&&!serachCity}}"><view class="hot-title">热门城市</view></block><block tt:if="{{hotCity.length>0&&!serachCity}}"><view class="hot-city"><block tt:for="{{hotCity}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['cityTrigger',['$0','hot'],[[['hotCity','',index]]]]]]]}}" class="hot-item" catchtap="__e">{{item[formatName]}}</view></block></view></block><block tt:if="{{!serachCity}}"><view class="citys"><block tt:for="{{sortItems}}" tt:for-item="city" tt:for-index="index" tt:key="index"><view hidden="{{!(city.isCity)}}" class="citys-row"><view class="citys-item-letter" id="{{'city-letter-'+(city.name==='#'?'0':city.name)}}">{{city.name}}</view><block tt:for="{{city.citys}}" tt:for-item="item" tt:for-index="inx" tt:key="inx"><view data-event-opts="{{[['tap',[['cityTrigger',['$0'],[[['sortItems','',index],['citys','',inx]]]]]]]}}" class="citys-item" bindtap="__e">{{item.cityName}}</view></block></view></block></view></block><block tt:if="{{serachCity}}"><view class="citys"><block tt:for="{{searchDatas}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="citys-row"><view data-event-opts="{{[['tap',[['cityTrigger',['$0'],[[['searchDatas','',index]]]]]]]}}" class="citys-item" bindtap="__e">{{item.name}}</view></view></block></view></block></scroll-view><block tt:if="{{!serachCity}}"><view class="city-indexs-view"><view class="city-indexs"><block tt:for="{{handleCity}}" tt:for-item="cityIns" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['cityindex',['$0'],[[['handleCity','',index,'forName']]]]]]]}}" hidden="{{!(cityIns.isCity)}}" class="city-indexs-text" bindtap="__e">{{''+cityIns.name+''}}</view></block></view></view></block></view>